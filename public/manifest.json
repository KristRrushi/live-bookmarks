{
  "manifest_version": 2,
  "name": "live-bookmarks",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo192.png",
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'sha256-zTgY3eZByp6zQmEL3b4cEtPLObecE6cS3xxu/ykOwMI='; object-src 'self'"
}
